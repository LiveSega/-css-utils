{
  "name": "@vikuloff/css-utils",
  "version": "1.0.1",
  "description": "Quick markup without pain",
  "homepage": "https://github.com/LiveSega/css-utils",
  "main": "dist/styles.css",
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "scripts": {
    "build": "node ./node_modules/less/bin/lessc ./src/styles.less ./dist/styles.css --clean-css=\"--s1 --advanced --compatibility=ie11\""
